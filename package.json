{
    "name": "heaps",
    "version": "0.1.0",
    "license": "MIT",

    "description": "TypeScript implementation of several heap data structures",

    "keywords": [ "heap", "typescript", "persistent", "binary heap", "pairing heap" ],

    "author": {
        "name": "gcnew"
    },

    "repository": {
        "type": "git",
        "url": "https://github.com/gcnew/heaps.git"
    },

    "files": [
        "src", "tsconfig.json"
    ],

    "devDependencies": {
        "pietr": "",

        "source-map-support": "",
        "chai": "",
        "istanbul": "",

        "@types/chai": ""
    },

    "scripts": {
